   <m:mi>c</m:mi>
  </m:msub>
  <m:mo>=</m:mo><m:mn>0,923</m:mn>
 </m:mrow>
</m:math> <newline/>Por lo tanto, <newline/> <m:math>
 <m:mrow>
  <m:msub>
   <m:msup>
    <m:mi>p</m:mi>
    <m:mo>′</m:mo>
   </m:msup>

   <m:mi>F</m:mi>
