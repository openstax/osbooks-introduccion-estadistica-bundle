            <tbody>
              <row>
                <entry>20–30</entry>
                <entry>15,0</entry>
                <entry>32,6</entry>
              </row>
              <row>
                <entry>31–40</entry>
                <entry>26,5</entry>
                <entry>32,6</entry>
