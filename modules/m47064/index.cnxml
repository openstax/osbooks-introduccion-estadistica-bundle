                <entry>51–60</entry>
                <entry>21,9</entry>
                <entry>36,6</entry>
              </row>
              <row>
                <entry>61–70</entry>
                <entry>21,0</entry>
                <entry>39,7</entry>
              </row>
            </tbody>
