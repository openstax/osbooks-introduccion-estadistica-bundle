<tgroup cols="6">
<thead>
  <row>
    <entry/>
    <entry>Deporte</entry>
    <entry>Sedán</entry>
    <entry>Utilitario</entry>
    <entry>Camioneta</entry>
    <entry>Van/suv</entry>
  </row>
</thead>
<tbody>
  <row>
    <entry>Familia</entry>
    <entry>5</entry>
    <entry>15</entry>
    <entry>35</entry>
    <entry>17</entry>
    <entry>28</entry>
  </row>
  <row>
    <entry>Sencillo</entry>
    <entry>45</entry>
    <entry>65</entry>
    <entry>37</entry>
    <entry>46</entry>
    <entry>7</entry>
  </row>
</tbody>
