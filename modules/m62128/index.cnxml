<row>
<entry/>
<entry>Automóvil, camión o furgoneta</entry>
<entry>Caminar</entry>
<entry>Transporte público</entry>
<entry>Otros</entry>
<entry>Totales</entry></row>
</thead>
<tbody>
<row>
<entry>Solo</entry>
<entry>0,7318</entry>
<entry/>
<entry/>
<entry/>
<entry/>
</row>
<row>
<entry>Acompañado</entry>
<entry>0,1332</entry>
<entry/>
<entry/>
<entry/>
<entry/>
</row>
<row>
<entry>Totales</entry>
<entry>0,8650</entry>
<entry>0,0390</entry>
<entry>0,0530</entry>
<entry>0,0430</entry>
<entry>1</entry></row>
