<entry>10,3</entry>
<entry>-1,8</entry>
</row>
<row>
<entry>8,1</entry>
<entry>11,3</entry>
<entry>-3,2</entry>
</row>
<row>
<entry>6,1</entry>
<entry>8,1</entry>
<entry>-2</entry>
</row>
<row>
