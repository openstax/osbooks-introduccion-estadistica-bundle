    <entry align="center" namest="c1" nameend="c2"><m:math><m:mi>P</m:mi><m:mo stretchy="false">(</m:mo><m:mi>x</m:mi><m:mo stretchy="false">)</m:mo><m:mo>=</m:mo><m:mfrac><m:mrow><m:mo>(</m:mo><m:mfrac linethickness="0"><m:mi>A</m:mi><m:mi>x</m:mi></m:mfrac><m:mo>)</m:mo><m:mo>(</m:mo><m:mfrac linethickness="0"><m:mrow><m:mi>N</m:mi><m:mo>−</m:mo><m:mi>A</m:mi></m:mrow><m:mrow><m:mi>n</m:mi><m:mo>−</m:mo><m:mi>x</m:mi></m:mrow></m:mfrac><m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo><m:mfrac linethickness="0"><m:mi>N</m:mi><m:mi>n</m:mi></m:mfrac><m:mo>)</m:mo></m:mrow></m:mfrac></m:math></entry>
