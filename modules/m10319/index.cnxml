    <entry align="center"><m:math><m:mi>b</m:mi><m:mo>=</m:mo><m:mfrac><m:mrow><m:mo stretchy="true">Σ</m:mo><m:mo>[</m:mo><m:mo>(</m:mo><m:mi>x</m:mi><m:mo>−</m:mo><m:mrow><m:mover><m:mi>x</m:mi><m:mo>¯</m:mo></m:mover></m:mrow><m:mo>)</m:mo><m:mo>(</m:mo><m:mi>y</m:mi><m:mo>−</m:mo><m:mrow><m:mover><m:mi>y</m:mi><m:mo>¯</m:mo></m:mover></m:mrow><m:mo>)</m:mo><m:mo>]</m:mo></m:mrow><m:mrow><m:mo>Σ</m:mo><m:msup><m:mrow><m:mo>(</m:mo><m:mi>x</m:mi><m:mo>−</m:mo><m:mrow><m:mover><m:mi>x</m:mi><m:mo>¯</m:mo></m:mover></m:mrow><m:mo>)</m:mo></m:mrow><m:mn>2</m:mn></m:msup></m:mrow></m:mfrac><m:mo>=</m:mo><m:mfrac><m:mrow><m:msub><m:mi>S</m:mi><m:mrow><m:mi>x</m:mi><m:mi>y</m:mi></m:mrow></m:msub></m:mrow><m:mrow><m:msub><m:mrow><m:mi>S</m:mi><m:mi>S</m:mi></m:mrow><m:mi>x</m:mi></m:msub></m:mrow></m:mfrac><m:mo>=</m:mo><m:msub><m:mi>r</m:mi><m:mrow><m:mi>y</m:mi><m:mo>,</m:mo><m:mi>x</m:mi></m:mrow></m:msub><m:mo>(</m:mo><m:mfrac><m:mrow><m:msub><m:mi>s</m:mi><m:mi>y</m:mi></m:msub></m:mrow><m:mrow><m:msub><m:mi>s</m:mi><m:mi>x</m:mi></m:msub></m:mrow></m:mfrac><m:mo>)</m:mo></m:math></entry>
